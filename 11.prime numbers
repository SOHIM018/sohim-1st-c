#include<stdio.h>
int main()
{
    int i, num, count=0;
    scanf("%d",&num);
    for(i=2; i<num; i++)
    {
        if(num%2==0)
        {
            count++;
            break;
        }
    }
    if(count==0)
        printf("is a prime number");
    else
        printf("is not a prime number");
    return 0;
}
